{
  "preset": "ts-jest",
  "transform": {
    "\\.[jt]s$": "babel-jest"
  },
  "rootDir": "../../",
  "testEnvironment": "node",
  "timers": "fake",
  "testMatch": ["**/?(*.)+(spec).[jt]s?(x)", "!**/?(*.)+(web).(spec).[jt]s?(x)"],
  "roots": ["<rootDir>/packages/core/src", "<rootDir>/packages/core/spec/service-worker/"],
  "modulePaths": ["<rootDir>"],
  "setupFiles": ["<rootDir>/jestSetup/index.ts", "<rootDir>/jestSetup/service-worker.js"]
}
